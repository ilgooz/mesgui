  "main": "electron.js",
  "scripts": {
    "build": "webpack",
    "serve": "webpack-dev-server --mode=development",
    "electron": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/circle_logo.icns --prune=true --out=release-builds",
